<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="iut.view.GraphiquesController">
    
    <!-- Onglets pour contenir les graphiques générés dynamiquement -->
    <TabPane fx:id="tabPane" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="50.0" 
             AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />

    <!-- Bouton retour positionné en bas -->
    <Button fx:id="buttonretour" mnemonicParsing="false" onAction="#handleButtonRetour" text="Retour" 
            AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" 
            maxWidth="150" />
</AnchorPane>
